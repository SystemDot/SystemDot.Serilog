<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>net6.0;net8.0</TargetFrameworks>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Version>1.0.1-beta</Version>
        <Title>SystemDot Serilog</Title>
        <Description>Utilities and extension for Serilog</Description>
        <Copyright>SystemDot</Copyright>
        <Authors>SystemDot, James Goodenough, Chris Sampson</Authors>
        <PackageId>SystemDot.Serilog</PackageId>
        <PackageTags>Serilog Formatting</PackageTags>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <Company>SystemDot</Company>
    </PropertyGroup>
    
    <ItemGroup>
        <PackageReference Include="Serilog" Version="2.0.0"/>
    </ItemGroup>

</Project>
